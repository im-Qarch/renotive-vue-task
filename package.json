{
  "name": "renotive-vue-js-mini-assignment",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.13.1",
  "description": "Renotive | VueJS Mini Assignment",
  "author": {
    "name": "Qarch",
    "email": "ali.carnoat@gmail.com"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/im-Qarch/renotive-vue-task.git"
  },
  "keywords": [
    "Vue",
    "Vite",
    "Typescript",
    "Pinia",
    "Tailwind"
  ],
  "scripts": {
    "build": "vite build --mode prod",
    "deploy": "gh-pages -d dist",
    "dev": "vite --port 3000",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "typecheck": "vue-tsc --noEmit",
    "postinstall": "npx simple-git-hooks",
    "sizecheck": "npx vite-bundle-visualizer"
  },
  "dependencies": {
    "@iconify-json/solar": "^1.1.9",
    "@unhead/vue": "^1.8.9",
    "@unocss/reset": "^0.58.2",
    "@vueuse/core": "^10.7.1",
    "@vueuse/head": "^2.0.0",
    "axios": "^1.6.3",
    "commitizen": "^4.3.0",
    "crypto-js": "^4.2.0",
    "js-cookie": "^3.0.5",
    "nanoid": "^4.0.2",
    "pinia": "^2.1.7",
    "pinia-plugin-store": "^2.2.0",
    "vue": "^3.4.3",
    "vue-barcode-reader": "^1.0.3",
    "vue-demi": "^0.14.6",
    "vue-i18n": "^9.8.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.1",
    "@iconify/vue": "^4.1.1",
    "@intlify/unplugin-vue-i18n": "^2.0.0",
    "@types/crypto-js": "^4.2.1",
    "@types/js-cookie": "^3.0.6",
    "@unocss/eslint-config": "^0.58.2",
    "@unocss/eslint-plugin": "^0.58.2",
    "@vitejs/plugin-vue": "^5.0.2",
    "@vue-macros/volar": "^0.18.8",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-plugin-format": "^0.1.0",
    "gh-pages": "^6.1.1",
    "https-localhost": "^4.7.1",
    "lint-staged": "^15.2.0",
    "naive-ui": "^2.38.1",
    "pnpm": "^8.13.1",
    "rollup": "^4.9.2",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3",
    "unocss": "^0.58.2",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-macros": "^2.7.7",
    "vite": "^5.0.10",
    "vite-bundle-visualizer": "^1.0.0",
    "vite-plugin-inspect": "^0.8.1",
    "vite-plugin-vue-devtools": "^7.0.2",
    "vite-plugin-webfont-dl": "^3.9.1",
    "vue-tsc": "^1.8.27"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
  },
  "lint-staged": {
    "*": "eslint --fix --no-warn-ignored"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
